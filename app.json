{
  "expo": {
    "name": "toeic-app",
    "slug": "quiz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/toeic-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/toeic-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/toeic-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chankiet1804.quizapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-sqlite",
      "expo-speech-recognition",
      "@react-native-voice/voice"
    ],
    "extra": {
      "eas": {
        "projectId": "9b8f626c-730f-41b0-a390-bbca8576983f"
      }
    }
  }
}
